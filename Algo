//return string length

def strlen(word):
    c = 0
    for i in word:
        c+=1
    return c

print(strlen("hello world"))

//Palindrome
def strrev(a):
    return(a[::-1])
def ispalin(b):
    c = strrev(b)
    if(c == b):
        print("Yes Palindrome")
    else:
        print("Not a Palindrome")
p="Man"
ispalin(p)
    
